{
    "name": "vite-engine",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "vite:dev": "vite --config vite.config.ts target/intermediate",
        "dev": "tsx --watch-path=src engine/main.ts",
        "build": "tsx engine/main.ts",
        "vite:build": "vite build --config vite.config.ts target/intermediate --outDir ../dist --emptyOutDir true",
        "vite:preview": "vite preview --config vite.config.ts target",
        "fullbuild": "npm run build && npm run vite:build"
    },
    "devDependencies": {
        "@types/node": "^18.19.63",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/styled-components": "^5.1.34",
        "@vitejs/plugin-react": "^4.3.3",
        "prettier": "^3.3.3",
        "rollup": "^4.24.3",
        "sass-embedded": "^1.80.6",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vite-plugin-radar": "^0.9.6"
    },
    "dependencies": {
        "@contentful/rich-text-react-renderer": "^16.0.0",
        "@contentful/rich-text-types": "^17.0.0",
        "@fontsource/nunito": "^5.1.0",
        "@fontsource/playfair-display": "^5.1.0",
        "@fortawesome/free-brands-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "bulma": "^1.0.2",
        "contentful": "^11.2.0",
        "pure-react-carousel": "^1.32.0",
        "react": "^18.3.1",
        "react-bulma-components": "^4.1.0",
        "react-dom": "^18.3.1",
        "styled-components": "^6.1.13"
    }
}
